@model IEnumerable<Emblishment_Management_System.Models.emsInspectionT>
@using Emblishment_Management_System.Models
@{
    ViewBag.Title = "GetInspectionReverseDetails";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Reverse Inspection
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>Transaction</a></li>
        <li class="active">Reverse Inspection</li>
    </ol>
    <style>
        form {
            display: inline;
        }
    </style>
</section>

<!-- Main content -->
<section class="content">
    <!-- Your Page Content Here -->


    <div class="box box-default">
        <input type="hidden" id="searchval" />
        <div class="box-body">
            <div class="row">
                <div class="col-lg-12 btn-container-right">
                    <div class="btn-group">
                        @using (Html.BeginForm("SaveReverse", "Inspection", FormMethod.Post))
                        {
                            <button type="submit" class="btn btn-primary"> &nbsp; Save &nbsp;</button>
                            <button type="button" class="btn btn-warning" onclick="location.href='@Url.Action("Index", "Home")'"> Cancel  </button>
                        } 
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="box box-default">
        <div class="box-header with-border">
            <h3 class="box-title">Reverse Inspection Details</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            </div>
            

        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="row">
                <div class="col-xs-12">
                    <div id="divPartialInspectView">
                        @{List<emsInspectionT> lstemsInspectionT = new List<emsInspectionT>(); }
                        @{ Html.RenderPartial("_PartialInspectionReverseDetails", Model); }
                    </div>

                    <!-- /.box -->
                </div>
            </div>
        </div>
    </div>
</section>





